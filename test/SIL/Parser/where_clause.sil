// RUN: %swift %s -emit-silgen | FileCheck %s

// rdar://16238241
// Make sure we can parse where clause with conformance & same-type requirements.

protocol Runcible {
  typealias Mince
  typealias Quince
}

struct Spoon<T: Runcible> {}

// CHECK: sil @foo : $@thin <T where T : Runcible, T == T.Mince> (@out Spoon<T>) -> ()
sil @foo : $@thin <T where T: Runcible, T == T.Mince> (@out Spoon<T>) -> () {
entry(%0 : $*Spoon<T>):
  return undef : $()
}
