// RUN: %swift %s -emit-silgen | FileCheck %s

sil_stage canonical

import Builtin
import Swift

// We cannot deserialize a SILDeclRef of kind GlobalAccessor, so this
// is not a proper addressor. But at least we can parse
// "[global_init]".
//
// CHECK-LABEL: g.MyVar.mutableAddressor : Swift.Int
// CHECK-NEXT: sil [global_init] @_TF1ga5MyVarSi

sil [global_init] @_TF1ga5MyVarSi : $@thin () -> Builtin.RawPointer {
bb0:
  %b = alloc_box $Int
  %p = address_to_pointer %b#1 : $*Int to $Builtin.RawPointer
  return %p : $Builtin.RawPointer
}
