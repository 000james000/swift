// RUN: %swift -emit-module %s -target x86_64-apple-darwin14.0.0 -module-name Swift -sil-serialize-all -module-link-name swift_stdlib_core -parse-as-library -parse-sil -parse-stdlib -o - | %sil-opt -verify -module-name=Swift

struct A {
  var f : @callee_owned () -> ()
}

sil @callee_check : $@thin (@callee_owned () -> ()) -> ()

sil @test1 : $@thin (@inout A) -> () {
bb0(%0 : $*A):
  %1 = struct_element_addr %0 : $*A, #f
  %2 = load %0 : $*A
  %3 = struct_extract %2 : $A, #f
  %4 = function_ref @callee_check : $@thin (@callee_owned () -> ()) -> ()
  apply %4(%3) : $@thin (@callee_owned () -> ()) -> ()
  %5 = tuple()
  return %5 : $()
}

class B {
  var f : @callee_owned () -> ()
}

sil @test2 : $@thin (B) -> () {
bb0(%0 : $B):
  %1 = ref_element_addr %0 : $B, #B.f
  %2 = tuple()
  return %2 : $()
}
