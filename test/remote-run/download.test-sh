RUN: %empty-directory(%t)
RUN: %empty-directory(%t-REMOTE)
RUN: %debug-remote-run --test-dir %t touch %t/output
RUN: ls %t/ | %FileCheck %s
RUN: ls %t-REMOTE/inout/ | %FileCheck %s

RUN: %empty-directory(%t)
RUN: %empty-directory(%t/nested)
RUN: %empty-directory(%t-REMOTE)
RUN: %debug-remote-run --test-dir %t touch %t/nested/output
RUN: ls %t/nested/ | %FileCheck %s
RUN: ls %t-REMOTE/inout/nested/ | %FileCheck %s

CHECK: {{^output$}}
