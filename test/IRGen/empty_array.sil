// RUN: %swift -emit-ir %s | FileCheck %s
// XFAIL: linux

sil_stage canonical

import Builtin

// CHECK-LABEL: define void @empty_array(%swift.opaque* noalias) {
// CHECK:         %1 = bitcast %swift.opaque* %0 to i8*
// CHECK:         %2 = getelementptr inbounds i8* %1, i64 0
sil @empty_array : $@thin (@in ()) -> () {
entry(%0 : $*()):
  %1 = integer_literal $Builtin.Word, 1
  %2 = index_addr %0 : $*(), %1 : $Builtin.Word
  return undef : $()
}
