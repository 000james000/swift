// RUN: %swift -emit-llvm -target x86_64-apple-darwin10 %s | FileCheck %s

import Builtin

// CHECK: define void @undefined() {
// CHECK: entry:
// CHECK:   store i64 undef, i64* undef, align 8
// CHECK:   store i8 undef, i8* undef, align 8
// CHECK:   store i8* undef, i8** undef, align 8
// CHECK:   ret void
// CHECK: }
sil @undefined : $() -> () {
entry:
  store undef to undef : $*(Builtin.Int64, Builtin.Int8, Builtin.RawPointer)
  return undef : $()
}
