BUILTIN_BINARY_PREDICATE(ICMP_UGT, "cmp_ugt",  OverloadedBuiltinKind::IntegerOrRawPointer)
BUILTIN_BINARY_PREDICATE(FCMP_OEQ, "fcmp_oeq", OverloadedBuiltinKind::Float)
