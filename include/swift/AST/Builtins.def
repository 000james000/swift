#ifndef BUILTIN_LOAD
#define BUILTIN_LOAD(Id, Name) BUILTIN(Id, Name)
#endif
BUILTIN_LOAD(Load, "load")
#undef BUILTIN_LOAD

/// Assign has type (T, Builtin.RawPointer) -> ()
