//
// This file defines macros used for macro-metaprogramming with attributes.
//
//===----------------------------------------------------------------------===//

#ifndef ATTR
#define ATTR(X)
#endif

// ATTR(name) - Invokes a macro for each attribute.
ATTR(assignment)
ATTR(cc)
ATTR(class_protocol)
ATTR(conversion)
ATTR(exported)
ATTR(infix)
ATTR(resilient)
ATTR(fragile)
ATTR(born_fragile)
ATTR(inout)
ATTR(auto_closure)
ATTR(thin)
ATTR(noreturn)
ATTR(asmname)
ATTR(prefix)
ATTR(postfix)
ATTR(objc)
ATTR(objc_block)
ATTR(iboutlet)
ATTR(ibaction)
ATTR(local_storage)
ATTR(transparent)

// SIL-specific attributes
ATTR(sil_self)

// Axle-specific attributes
ATTR(kernel)
ATTR(vertex)
ATTR(fragment)

#undef ATTR
