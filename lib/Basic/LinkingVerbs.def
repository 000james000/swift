// This file lists the linking verbs that will prevent splitting of the first
// piece of an Objective-C selector.
// ===---------------------------------------------------------------------===//
#ifndef LINKING_VERB
#  error Define LINKING_VERB(Word) before including this file.
#endif

LINKING_VERB(did)
LINKING_VERB(is)
LINKING_VERB(has)
LINKING_VERB(needs)
LINKING_VERB(should)
LINKING_VERB(was)
LINKING_VERB(will)

#undef LINKING_VERB
