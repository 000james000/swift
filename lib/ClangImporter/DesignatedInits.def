DESIGNATED_INIT(NSRulerView, (2, "initWithScrollView", "orientation"))

DESIGNATED_INIT(NSTableColumn, (1, "initWithIdentifier"))

DESIGNATED_INIT(NSTextAttachment, (1, "initWithFileWrapper"))

DESIGNATED_INIT(NSTextBlock, (0, "init"))

DESIGNATED_INIT(NSTextContainer, (1, "initWithContainerSize"))

