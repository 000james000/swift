//
// This file defines the database of the designated initializers for
// various Objective-C classes. Eventually, all of this information
// should migrate to the corresponding header using
// OBJC_DESIGNATED_INITIALIZER or NS_DESIGNATED_INITIALIZER.
// ===---------------------------------------------------------------------===//

// DESIGNATED_INIT(ClassName, Selector)
// 
//   Describes a designated initializer with at least one argument.
//
//   ClassName: The name of the class.
//
//   Selector: A parenthesized sequence containing the # of selector
//   pieces followed by strings containing the selector pieces.

// ObjectiveC
DESIGNATED_INIT(NSObject, (0, "init"))

// AppKit
DESIGNATED_INIT(NSCell, (0, "init"))
DESIGNATED_INIT(NSCell, (1, "initWithCoder"))
DESIGNATED_INIT(NSCell, (1, "initTextCell"))
DESIGNATED_INIT(NSCell, (1, "initImageCell"))

DESIGNATED_INIT(NSControl, (1, "initWithFrame"))

DESIGNATED_INIT(NSCursor, (2, "initWithImage", "hotSpot"))

DESIGNATED_INIT(NSDocument, (0, "init"))

DESIGNATED_INIT(NSDocumentController, (0, "init"))

DESIGNATED_INIT(NSDraggingImageComponent, (1, "initWithKey"))

DESIGNATED_INIT(NSDraggingItem, (1, "initWithPasteboardWriter"))

DESIGNATED_INIT(NSFontManager, (0, "init"))

DESIGNATED_INIT(NSFormCell, (1, "initTextCell"))

DESIGNATED_INIT(NSGradient, (3, "initWithColors", "atLocations", "colorSpace"))

DESIGNATED_INIT(NSImageCell, (0, "init"))
DESIGNATED_INIT(NSImageCell, (1, "initWithCoder"))
DESIGNATED_INIT(NSImageCell, (1, "initTextCell"))
DESIGNATED_INIT(NSImageCell, (1, "initImageCell"))

DESIGNATED_INIT(NSLayoutManager, (0, "init"))

DESIGNATED_INIT(NSMatrix, 
                (5, "initWithFrame", "mode", "cellClass", "numberOfRows", 
