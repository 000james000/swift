DESIGNATED_INIT(NSTextView, (2, "initWithFrame", "textContainer"))

DESIGNATED_INIT(NSUserDefaultsController, 
                (2, "initWithDefaults", "initialValues"))

DESIGNATED_INIT(NSTextView, (1, "initWithFrame"))

DESIGNATED_INIT(NSWindow, 
                (4, "initWithContentRect", "styleMask", "backing", "defer"))

DESIGNATED_INIT(NSWindowController, (1, "initWithWindow"))

// UIKit
DESIGNATED_INIT(UIDocument, (1, "initWithFileURL"))

#undef DESIGNATED_INIT
